{
    "manifest_version": 2,
  
    "name": "Polyjuicer",
    "description": "Polyjuicer JWT Hot-Switcher",
    "version": "0.0.1",
  
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Polyjuicer"
    },  
    "icons": {},
    "permissions": [
        "webRequest", 
        "webRequestBlocking",
        "*://localhost/",
        "*://*.gofore.com/",
        "cookies"
    ],
    "background": {
        "scripts": ["background.js"]
    }
  }